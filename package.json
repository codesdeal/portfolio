{
  "name": "_themename",
  "version": "1.0.0",
  "description": "An Interactive Responsive Agency/Marketing WordPress Theme",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "gulp",
    "build": "gulp build --prod",
    "bundle": "gulp bundle --prod",
    "lint:js": "eslint 'src/assets/js/**/*.js'",
    "lint:scss": "stylelint 'src/assets/scss/**/*.scss'",
    "lint": "npm run lint:js && npm run lint:scss",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codesdeal/myfirstwptheme.git"
  },
  "keywords": [
    "light",
    "dark",
    "two-columns",
    "right-sidebar",
    "responsive-layout",
    "accessibility-ready"
  ],
  "author": "Abu Bakkar Siddique",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/codesdeal/myfirstwptheme/issues"
  },
  "homepage": "https://github.com/codesdeal/myfirstwptheme#readme",
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-typescript": "^7.22.0",
    "@fortawesome/fontawesome-free": "^6.4.0",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@testing-library/dom": "^9.3.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@wordpress/eslint-plugin": "^14.0.0",
    "aos": "^2.3.4",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "bootstrap": "^5.2.3",
    "browser-sync": "^2.29.3",
    "critical": "^5.0.0",
    "css-loader": "^6.11.0",
    "cssnano": "^6.0.1",
    "del": "^7.0.0",
    "eslint": "^8.41.0",
    "file-loader": "^6.2.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-cli": "^3.0.0",
    "gulp-dart-sass": "^1.1.0",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-postcss": "^9.0.1",
    "gulp-replace": "^1.1.4",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-terser": "^2.1.0",
    "gulp-webp": "^4.0.1",
    "gulp-zip": "^5.1.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.4.23",
    "sass": "^1.86.3",
    "sass-loader": "^13.3.3",
    "style-loader": "^3.3.4",
    "stylelint": "^15.6.2",
    "stylelint-config-standard-scss": "^9.0.0",
    "terser": "^5.17.4",
    "terser-webpack-plugin": "^5.3.9",
    "typescript": "^5.0.4",
    "vinyl-named": "^1.1.0",
    "webpack": "^5.84.1",
    "webpack-cli": "^5.1.1",
    "webpack-stream": "^7.0.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "isotope-layout": "^3.0.6",
    "jquery": "^3.7.0",
    "normalize.css": "^8.0.1",
    "swiper": "^9.3.2",
    "yargs": "^17.7.2"
  },
  "lint-staged": {
    "src/assets/js/**/*.js": [
      "eslint --fix"
    ],
    "src/assets/scss/**/*.scss": [
      "stylelint --fix"
    ],
    "*.{js,jsx}": [
      "eslint --max-warnings=0 --fix"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/js/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "<rootDir>/tests/js/__mocks__/styleMock.js",
      "\\.(jpg|jpeg|png|gif|webp|svg)$": "<rootDir>/tests/js/__mocks__/fileMock.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/vendor/"
    ],
    "collectCoverageFrom": [
      "src/assets/js/**/*.js",
      "!**/node_modules/**"
    ]
  }
}
