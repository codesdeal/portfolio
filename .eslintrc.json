{
    "extends": [
        "plugin:@wordpress/eslint-plugin/recommended",
        "plugin:prettier/recommended"
    ],
    "env": {
        "browser": true,
        "es6": true,
        "jquery": true
    },
    "parserOptions": {
        "ecmaVersion": 2022,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "warn",
        "no-unused-vars": "warn",
        "prefer-const": "warn",
        "jsdoc/require-param": "off",
        "jsdoc/require-returns": "off",
        "prettier/prettier": "error"
    },
    "plugins": ["prettier"]
}